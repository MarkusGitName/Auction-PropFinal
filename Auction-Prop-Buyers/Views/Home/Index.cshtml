        <!-- Our Services End -->
        <section id="price-drop" class="block">
            @{
                ICollection<Property> PropLys = APILibrary.APIMethods.APIGetALL<ICollection<Property>>("Properties");

                List<Property> DisplyPropLys = new List<Property>();



                <div class="container">
                    <header class="section-title">
                        <h2>Upcoming Auctions</h2>
                        <a href="properties-listing.html" class="link-arrow">All Properties</a>
                    </header>
                </div>
                <div class="row">
                    @foreach (Property prop in PropLys)
                    {

                        if (prop.Auction != null)
                        {
                            DisplyPropLys.Add(prop);
                        }


                    }

                    @{
                        IEnumerable<Property> query = from prop in DisplyPropLys
                                                      orderby prop.Auction.StartTime
                                                      select prop;

                        int count = 0;
                        foreach (Property auc in query)
                        {
                            while (count < 4)
                            {
                                <div class="col-md-3 col-sm-6" >
                                    <div class="property">
                                        <a href="property-detail.html"style="min-height:100px; max-width:300px; padding:15px">
                                            <div class="property-image">
                                                <img alt="" src=@auc.PropertyPhotos.First().PropertyPhotoPath >
                                            </div>
                                            <div class="overlay">
                                                <div class="info">
                                                    <div class="tag date">@auc.Auction.StartTime</div>
                                                    <h3>@auc.Address</h3>
                                                    <figure>@auc.City, @auc.Province</figure>
                                                </div>
                                                <ul class="additional-info">
                                                    <li>
                                                        <header>Area:</header>
                                                        <figure>@auc.BuildingArea<sup>2</sup></figure>
                                                    </li>
                                                    <li>
                                                        <header>Beds:</header>
                                                        <figure>@auc.BedRooms</figure>
                                                    </li>
                                                    <li>
                                                        <header>Baths:</header>
                                                        <figure>@auc.BathRooms</figure>
                                                    </li>
                                                    <li>
                                                        <header>Garages:</header>
                                                        <figure>@auc.Garages</figure>
                                                    </li>
                                                </ul>
                                            </div>
                                        </a>
                                    </div><!-- /.property -->
                                </div><!-- /.col-md-3 -->
                                count++;
                            }
                        }
                    }

                </div>

            }



        </section>



