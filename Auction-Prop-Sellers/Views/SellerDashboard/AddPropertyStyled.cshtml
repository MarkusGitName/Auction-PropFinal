                                                        @Html.CheckBoxFor(model => model.A_C, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>

                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.PetsAllowed, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.PetsAllowed, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.Garden, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.Garden, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.CableTV, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.CableTV, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>


                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.Terrace, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.Terrace, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>

                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.wi_fi, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.wi_fi, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.Fibre, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.Fibre, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.Pool, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">

                                                        @Html.CheckBoxFor(model => model.Pool, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>

                                                <div class="col-md-3" style="Padding:10px">
