                                                        @Html.CheckBoxFor(model => model.Beach, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.Gerage, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.Gerage, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
                                                    <div style="float:left">
                                                        @Html.LabelFor(model => model.RoofTarrace, htmlAttributes: new { @class = "control-label col-md-2" })
                                                    </div>
                                                    <div style="float:right">
                                                        @Html.CheckBoxFor(model => model.RoofTarrace, new { htmlAttributes = new { @class = "form-control" } })
                                                    </div>
                                                </div>
                                                <div class="col-md-3" style="Padding:10px">
